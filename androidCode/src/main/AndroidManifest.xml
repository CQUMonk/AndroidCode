        <activity
            android:name=".Ball.FollowFingerBall"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".prime.PrimeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".CreazyCat.CreazyCatActivity"
            android:label="@string/title_activity_creazy_cat" >
        </activity>
        <activity
            android:name=".BarCode.BarCodeActivity"
            android:label="@string/title_activity_bar_code" >
        </activity>
        <activity
            android:name="com.zxing.activity.CaptureActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:windowSoftInputMode="stateAlwaysHidden" >
        </activity>
        <activity
            android:name=".Game2048.Game2048Activity"
            android:label="2048" >
        </activity>
        <activity
            android:name=".login.loginActivity"
            android:label="@string/title_activity_login" >

        </activity>
        <activity
            android:name=".downLoadManager.DownLoadActivity"
            android:label="@string/title_activity_down_load" >
        </activity>
        <service android:name=".downLoadManager.DownLoadService"/>

        <activity android:name=".tabs.TabsActivity"
            android:label="TabsActivity"/>

    </application>

</manifest>
