<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cqu.cqumonk.androidcode"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.CAMERA"/><uses-permission android:name="android.permission.VIBRATE" />
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.PassParams"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ui.QQslide_menu"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ui.Weixin"
            android:label="@string/weixin"
            android:logo="@drawable/dekesite"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ui.FollowFingerBall"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ui.Cal"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".ui.CreazyCat"
            android:label="@string/title_activity_creazy_cat" >
        </activity>
        <activity
            android:name=".ui.BarCode"
            android:label="@string/title_activity_bar_code" >
        </activity>
        <activity
            android:configChanges="orientation|keyboardHidden"
            android:name="com.zxing.activity.CaptureActivity"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:windowSoftInputMode="stateAlwaysHidden" >
        </activity>
    </application>

</manifest>